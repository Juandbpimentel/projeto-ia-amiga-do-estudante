# Local development environment variables
# Copy this file to `.env` and configure values for local development.
# WARNING: Never commit a `.env` file with secrets to the repo; this is only a template.
REQUIRE_GOOGLE_CREDENTIALS=false
BACKEND_URL=http://localhost:8000
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000
ALLOW_ORIGINS=http://localhost:3000
# For local dev with Docker Compose: redis://127.0.0.1:6379/0
REDIS_URL=redis://127.0.0.1:6379/0
# Google API key/service account stub (for tests only)
GOOGLE_API_KEY=
GOOGLE_SERVICE_ACCOUNT_JSON=